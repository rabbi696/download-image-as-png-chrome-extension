{
  "manifest_version": 3,
  "name": "Download as PNG",
  "version": "1.0",
  "description": "Download any image as a PNG file.",
  "permissions": [
    "contextMenus",
    "downloads",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
